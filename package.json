{
  "name": "medtech-qms-automation",
  "version": "1.0.0",
  "description": "Medical Device QMS Automation System with Multi-Agent Architecture",
  "main": "dist/main/main.js",
  "scripts": {
    "build:renderer": "esbuild src/renderer/App.tsx --bundle --outfile=dist/renderer/bundle.js --platform=browser --target=es2020",
    "build:preload": "tsc src/preload.ts --outDir dist --esModuleInterop --skipLibCheck",
    "build:main": "tsc -p tsconfig.json",
    "build": "npm run build:main && npm run build:renderer",
    "start": "npm run build && electron dist/main/main.js",
    "dev": "tsc -w & electron .",
    "lint": "tsc --noEmit",
    "test": "vitest run"
  },
  "keywords": [
    "qms",
    "medical-device",
    "iso13485",
    "electron",
    "langchain"
  ],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "devDependencies": {
    "@types/node": "^25.0.7",
    "@types/react": "^19.2.8",
    "@types/react-dom": "^19.2.3",
    "@types/uuid": "^10.0.0",
    "electron": "^39.2.7",
    "esbuild": "^0.27.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "vitest": "^4.0.17"
  },
  "dependencies": {
    "@langchain/classic": "^1.0.8",
    "@langchain/community": "^1.1.3",
    "@langchain/core": "^1.1.12",
    "@langchain/google-genai": "^2.1.7",
    "@langchain/langgraph": "^1.0.15",
    "@types/bcryptjs": "^2.4.6",
    "@types/better-sqlite3": "^7.6.13",
    "@types/node-schedule": "^2.1.8",
    "bcryptjs": "^3.0.3",
    "better-sqlite3": "^12.6.0",
    "docxtemplater": "^3.67.6",
    "dotenv": "^17.2.3",
    "fs-extra": "^11.3.3",
    "google-auth-library": "^9.15.1",
    "googleapis": "^170.0.0",
    "langchain": "^1.2.7",
    "mammoth": "^1.11.0",
    "node-schedule": "^2.1.1",
    "pdf-parse": "^2.4.5",
    "pino": "^10.1.1",
    "pino-pretty": "^13.1.3",
    "pizzip": "^3.2.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "uuid": "^13.0.0",
    "zod": "^4.3.5"
  }
}
